{
  "expo": {
    "name": "weather_app",
    "description": "This project is really great.",
    "slug": "weather_app",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [ "ios", "android" ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/sun.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.caleblogan.weatherApp"
    },
    "android": {
      "package": "com.caleblogan.weatherApp"
    }
  }

}
